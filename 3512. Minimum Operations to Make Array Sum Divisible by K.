class Solution {
    public static int getCount(int num,int k) {
        int count = 0;
        while(num % k != 0) {
            num--;count++;
        }
        return count;
    }
    public int minOperations(int[] nums, int k) {
        int sum = Arrays.stream(nums).sum();
        if(sum % k == 0) return 0;
        return getCount(sum,k);
    }
}
